
<h1>Overall Status : Success <sup>
    <ac:emoticon ac:name="tick"/>
  </sup>
</h1>
<hr/>
<br/>
{{#with summary}}
<ac:structured-macro ac:macro-id="03c1d490-b119-445b-b91c-52fd0f7c346d" ac:name="section" ac:schema-version="1">
  <ac:rich-text-body>
    <p class="auto-cursor-target">
      <br/>
    </p>
  
    <ac:structured-macro ac:macro-id="summary_header_tbl" ac:name="column" ac:schema-version="1">
      <ac:parameter ac:name="width">20px</ac:parameter>
        <ac:rich-text-body>
            <p>
            <strong>
                <strong title="">Collection :</strong>
            </strong>
            </p>
            {{#if collection.description}}
            <p>Description :</p>
            {{/if}} 
            <p>Total run duration:</p>
            <p>Total data received:</p>
            <p>Average response time:</p>
            <p>Executed Time :</p>
        </ac:rich-text-body>
    </ac:structured-macro>
    <p class="auto-cursor-target"><br/></p>
    <p class="auto-cursor-target"><br/></p>
    <ac:structured-macro ac:macro-id="summary_header_details_tbl" ac:name="column" ac:schema-version="1">
      <ac:parameter ac:name="width">20px</ac:parameter>
      <ac:rich-text-body>
        <p>{{collection.name}}</p>
        {{#if collection.description}}
        <p>{{collection.description}}</p>
        {{/if}} 
        <p>{{duration}}</p>
        <p>{{responseTotal}}</p>
        <p>{{responseAverage}}(approx)</p>
{{/with}}
        <p>{{timestamp}}</p>
      </ac:rich-text-body>
    </ac:structured-macro>
    <p class="auto-cursor-target"><br/></p>

    <p class="auto-cursor-target">
      <br/>
    </p>
    <ac:structured-macro ac:macro-id="6d5fba68-44ab-4c1d-947b-cadd6afbd666" ac:name="column" ac:schema-version="1">
      <ac:parameter ac:name="width">40%</ac:parameter>
      <ac:rich-text-body>
        <p>
          <br/>
        </p>
      </ac:rich-text-body>
    </ac:structured-macro>
    <p class="auto-cursor-target">
      <br/>
    </p>
</ac:rich-text-body>
</ac:structured-macro>

   {{#with summary}}
        <h1>Total Failures : <span style="color: rgb(51,153,102);">
            <strong>{{failures.length}}</strong>
        </span>
        </h1>
        {{#with stats}}
            <table class="relative-table" style="width: 36.2209%;">
            <colgroup>
                <col style="width: 54.8633%;"/>
                <col style="width: 23.3119%;"/>
                <col style="width: 21.5032%;"/>
            </colgroup>
            <tbody>
                <tr>
                <th>
                    <br/>
                </th>
                <th style="text-align: center;">Total</th>
                <th style="text-align: center;">Failed</th>
                </tr>
                <tr>
                <td colspan="1">
                    <span style="color: rgb(51,51,51);">Iterations</span>
                </td>
                <td colspan="1">
                    <span style="color: rgb(51,51,51);">{{iterations.total}}</span>
                </td>
                <td colspan="1">
                    <span style="color: rgb(51,51,51);">{{iterations.failed}}</span>
                </td>
                </tr>
                <tr>
                <td colspan="1">
                    <span style="color: rgb(51,51,51);">Requests</span>
                </td>
                <td colspan="1">
                    <span style="color: rgb(51,51,51);">{{requests.total}}</span>
                </td>
                <td colspan="1">
                    <span style="color: rgb(51,51,51);">{{requests.failed}}</span>
                </td>
                </tr>
                <tr>
                <td colspan="1">
                    <span style="color: rgb(51,51,51);">Prerequest Scripts</span>
                </td>
                <td colspan="1">
                    <span style="color: rgb(51,51,51);">{{prerequestScripts.total}}</span>
                </td>
                <td colspan="1">
                    <span style="color: rgb(51,51,51);">{{prerequestScripts.failed}}</span>
                </td>
                </tr>
                <tr>
                <td colspan="1">
                    <span style="color: rgb(51,51,51);">Test Scripts</span>
                </td>
                <td colspan="1">
                    <span style="color: rgb(51,51,51);">{{testScripts.total}}</span>
                </td>
                <td colspan="1">
                    <span style="color: rgb(51,51,51);">{{testScripts.failed}}</span>
                </td>
                </tr>
                <tr>
                <td colspan="1">
                    <span style="color: rgb(51,51,51);">Assertions</span>
                </td>
                <td colspan="1">
                    <span style="color: rgb(51,51,51);">{{assertions.total}}</span>
                </td>
                <td colspan="1">
                    <span style="color: rgb(51,51,51);">{{assertions.failed}}</span>
                </td>
                </tr>
            </tbody>
            </table>
        {{/with}}
    {{/with}}

    {{#each aggregations}}
        {{#if parent.name}}
        <div class="panel-group" id="collapse-folder-{{parent.id}}" role="tablist" aria-multiselectable="true">
            <div role="tab" id="folderHead-{{parent.id}}">
                <h4 style="font-size: 18px;" class="panel-title"><a data-toggle="collapse" data-parent="#accordion" href="#folderData-{{parent.id}}" aria-controls="collapseOne"><strong>{{parent.name}}</strong></a></h4>
            </div>
            <br/>
            <div id="folderData-{{parent.id}}" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="folderHead-{{parent.id}}">
            {{> aggregations}}
            </div>
        </div>
        {{else}}
        {{> aggregations}}
        {{/if}}
    {{/each}}


<h1>API Requests</h1>
<p>
<br/>
</p>
{{#*inline "aggregations"}}
    {{#each executions}}
      {{#with request}}
      <p style="margin-left: 30.0px;">Name : {{url}}</p>
      {{/with}}
      <table class="relative-table wrapped" style="width: 14.2442%;margin-left: 30.0px;">
        <colgroup> <col style="width: 48.668%;"/> <col style="width: 50.7172%;"/> </colgroup>
        <tbody style="margin-left: 30.0px;">
          <tr style="margin-left: 30.0px;">
            <th style="margin-left: 30.0px;">Passed</th>
            <th style="margin-left: 30.0px;">Fail</th>
          </tr>
          <tr style="margin-left: 30.0px;">
            <td colspan="1" style="margin-left: 30.0px;">{{cumulativeTests.passed}}</td>
            <td colspan="1" style="margin-left: 90.0px;">{{cumulativeTests.failed}}</td>
          </tr>
        </tbody>
      </table>
      <p style="margin-left: 30.0px;">
        <br/>
      </p>

        <ac:structured-macro ac:macro-id="collapse-request-{{item.id}}" ac:name="expand" ac:schema-version="1">
        <ac:parameter ac:name="title">{{item.name}} - Test Details</ac:parameter>
        <ac:rich-text-body>
              <div class="panel panel-default">
                  <div id="requestData-{{item.id}}" class="panel-collapse collapse in" role="tabpanel" aria-labelledby="requestHead-{{item.id}}">
                      <div class="panel-body">
                          {{#with request}}
                              {{#if description.content}}
                                  <div class="col-md-4">Description:{{description.content}}</div>
                                  <div class="col-md-12">&nbsp;</div>
                              {{/if}}

                              <div class="col-md-4">Method: {{method}}</div>
                              <div class="col-md-4">URL: <a href="{{url}}" target="_blank">{{url}}</a></div>
                          {{/with}}

                          <div class="col-md-12">&nbsp;</div>
                          {{#if requestError}}
                              <div class="col-md-4"><b>Error</b></div><div class="col-md-8"><b>There was an error sending this request</b></div>
                              <div class="col-md-4"><b>Error code</b></div><div class="col-md-8"><b>{{requestError.code}}</b></div>
                              {{#if requestError.connect}}
                                  <div class="col-md-4"><b>Details</b></div><div class="col-md-8"><b>A connection to the host could not be established within the stipulated timeout</b></div>
                              {{/if}}
                          {{/if}}

                          <div class="col-md-12">&nbsp;</div>
                          <div class="col-md-4">Mean time per request: {{mean.time}}</div>
                          <div class="col-md-4">Mean size per request: {{mean.size}}</div>
                          <div class="col-md-4">Total passed tests:{{cumulativeTests.passed}}</div>
                          <div class="col-md-4">Total failed tests: {{cumulativeTests.failed}}</div><br/>

                          <div class="col-md-12">&nbsp;</div>
                          <br/><div class="col-md-4">Status code : {{response.code}}</div>

                          {{#if assertions.length}}
                              <div class="col-md-12">&nbsp;</div>
                              <div class="col-md-4">Validated Scenarios &amp; Test Results</div>
                              <div class="col-md-8">
                                  <table class="table table-responsive table-condensed">
                                      <thead><tr><th>Name</th><th>Pass count</th><th>Fail count</th></tr></thead>
                                      <tbody>{{#each assertions}}<tr><td>{{this.name}}</td><td>{{this.passed}}</td><td>{{this.failed}}</td></tr>{{/each}}</tbody>
                                  </table>
                              </div>
                          {{/if}}
                      </div>
                  </div>
              </div>
          </ac:rich-text-body>
        </ac:structured-macro>
    {{/each}}
{{/inline}}
